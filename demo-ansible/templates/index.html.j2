        <div class="main__center center">
            <h1 class="main__title title title_big">This website certificate was enrolled using the Horizon Ansible collection</h1>
        </div>
    </div>

    <table class="main__wrap">
        <tr>
            <td class="title__color">DN:</td>
            <td class="right ">{{ enrolled_certificate['certificate']['dn'] }}</td>
        </tr>
        <tr>
            <td class="title__color">Issuer DN:</td>
            <td class="right">{{ enrolled_certificate['certificate']['issuer'] }}</td>
        </tr>
        <tr>
            <td class="title__color">Not Before:</td>
            <td class="right">{{ '%d %b %y %H:%M %Z' | strftime(enrolled_certificate['certificate']['notBefore']|int / 1000) }}</td>
        </tr>
        <tr>
            <td class="title__color">Not After:</td>
            <td class="right">{{ '%d %b %y %H:%M %Z' | strftime(enrolled_certificate['certificate']['notAfter']|int / 1000) }}</td>
        </tr>
        <tr>
            <td class="title__color">Serial:</td>
            <td class="right">0x{{ enrolled_certificate['certificate']['serial'] }}</td>
        </tr>
        <tr>
            <td class="title__color">SANS:</td>
            <td class="right">{{ enrolled_certificate['certificate']['subjectAlternateNames'] }}</td>
        </tr>

    </table>

</div>
